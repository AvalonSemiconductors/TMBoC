library (posit_unit) {
  comment                        : "";
  delay_model                    : table_lookup;
  simulation                     : false;
  capacitive_load_unit (1,pF);
  leakage_power_unit             : 1pW;
  current_unit                   : "1A";
  pulling_resistance_unit        : "1kohm";
  time_unit                      : "1ns";
  voltage_unit                   : "1v";
  library_features(report_delay_calculation);

  input_threshold_pct_rise : 50;
  input_threshold_pct_fall : 50;
  output_threshold_pct_rise : 50;
  output_threshold_pct_fall : 50;
  slew_lower_threshold_pct_rise : 20;
  slew_lower_threshold_pct_fall : 20;
  slew_upper_threshold_pct_rise : 80;
  slew_upper_threshold_pct_fall : 80;
  slew_derate_from_library : 1.0;


  nom_process                    : 1.0;
  nom_temperature                : 25.0;
  nom_voltage                    : 1.80;

  lu_table_template(template_1) {
    variable_1 : total_output_net_capacitance;
    index_1 ("0.00050,  0.00161,  0.00520,  0.01675,  0.05400,  0.17409,  0.56123");
  }
  lu_table_template(template_2) {
    variable_1 : total_output_net_capacitance;
    index_1 ("0.00050,  0.00161,  0.00520,  0.01675,  0.05400,  0.17409,  0.56123");
  }
  type ("io_in") {
    base_type : array;
    data_type : bit;
    bit_width : 3;
    bit_from : 2;
    bit_to : 0;
  }
  type ("io_out") {
    base_type : array;
    data_type : bit;
    bit_width : 4;
    bit_from : 3;
    bit_to : 0;
  }

  cell ("posit_unit") {
    pin("clk") {
      direction : input;
      capacitance : 0.0134;
    }
    pin("rst") {
      direction : input;
      capacitance : 0.0028;
      timing() {
        related_pin : "clk";
        timing_type : hold_rising;
	rise_constraint(scalar) {
          values("-0.08009");
	}
	fall_constraint(scalar) {
          values("-0.05652");
	}
      }
      timing() {
        related_pin : "clk";
        timing_type : setup_rising;
	rise_constraint(scalar) {
          values("0.90062");
	}
	fall_constraint(scalar) {
          values("1.25135");
	}
      }
    }
    pin("vccd1") {
      direction : input;
      capacitance : 0.0000;
    }
    pin("vssd1") {
      direction : input;
      capacitance : 0.0000;
    }
    bus("io_in") {
      bus_type : io_in;
      direction : input;
      capacitance : 0.0000;
    pin("io_in[2]") {
      direction : input;
      capacitance : 0.0034;
      timing() {
        related_pin : "clk";
        timing_type : hold_rising;
	rise_constraint(scalar) {
          values("0.16351");
	}
	fall_constraint(scalar) {
          values("0.00286");
	}
      }
      timing() {
        related_pin : "clk";
        timing_type : setup_rising;
	rise_constraint(scalar) {
          values("-0.07640");
	}
	fall_constraint(scalar) {
          values("0.13847");
	}
      }
    }
    pin("io_in[1]") {
      direction : input;
      capacitance : 0.0024;
      timing() {
        related_pin : "clk";
        timing_type : hold_rising;
	rise_constraint(scalar) {
          values("-0.00407");
	}
	fall_constraint(scalar) {
          values("-0.20205");
	}
      }
      timing() {
        related_pin : "clk";
        timing_type : setup_rising;
	rise_constraint(scalar) {
          values("1.14552");
	}
	fall_constraint(scalar) {
          values("1.21393");
	}
      }
    }
    pin("io_in[0]") {
      direction : input;
      capacitance : 0.0027;
      timing() {
        related_pin : "clk";
        timing_type : hold_rising;
	rise_constraint(scalar) {
          values("-0.21006");
	}
	fall_constraint(scalar) {
          values("-0.32324");
	}
      }
      timing() {
        related_pin : "clk";
        timing_type : setup_rising;
	rise_constraint(scalar) {
          values("1.42353");
	}
	fall_constraint(scalar) {
          values("1.24288");
	}
      }
    }
    }
    bus("io_out") {
      bus_type : io_out;
      direction : output;
      capacitance : 0.0000;
    pin("io_out[3]") {
      direction : output;
      capacitance : 0.0334;
      timing() {
        related_pin : "clk";
        timing_type : rising_edge;
	cell_rise(template_1) {
          values("0.92235,0.92581,0.93552,0.96058,1.03152,1.25433,1.97537");
	}
	rise_transition(template_1) {
          values("0.01853,0.02163,0.03069,0.05937,0.15611,0.47622,1.51017");
	}
	cell_fall(template_2) {
          values("0.92402,0.92733,0.93628,0.95547,0.99651,1.10100,1.42153");
	}
	fall_transition(template_2) {
          values("0.02010,0.02209,0.02758,0.04187,0.08051,0.20690,0.63706");
	}
      }
    }
    pin("io_out[2]") {
      direction : output;
      capacitance : 0.0334;
    }
    pin("io_out[1]") {
      direction : output;
      capacitance : 0.0334;
    }
    pin("io_out[0]") {
      direction : output;
      capacitance : 0.0334;
    }
    }
  }

}
